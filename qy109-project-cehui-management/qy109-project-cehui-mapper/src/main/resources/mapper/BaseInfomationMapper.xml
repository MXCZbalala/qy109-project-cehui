  <select id="qureyMapping_unit" parameterType="com.aaa.cehui.model.MappingUnit" resultType="com.aaa.cehui.model.MappingUnit">
    select * from t_mapping_unit where user_id=#{userId}
  </select>
  <select id="fuzzyUnitName" resultMap="BaseResultMap" parameterType="java.lang.String" >
    SELECT id, unit_name, owned_district, qualification_level, register_address
    FROM t_mapping_unit
    <where>
      1=1
      <if test="unitName != null and unitName != '' " >
        AND unit_name like CONCAT('%', CONCAT(#{unitName, jdbcType=VARCHAR},'%'))
      </if>
      <if test="ownedDistrict != null and ownedDistrict != '' ">
        AND owned_district=#{ownedDistrict, jdbcType=VARCHAR}
      </if>
      <if test="qualificationLevel != null and qualificationLevel != '' ">
        AND qualification_level=#{qualificationLevel, jdbcType=VARCHAR}
      </if>
    </where>
    ORDER BY id ASC
  </select>
  <select id="queryBaiMingDanByUnitStatus" parameterType="com.aaa.cehui.model.MappingUnit" resultType="com.aaa.cehui.model.MappingUnit">
  select id,unit_name,register_address,qualification_level,certificate_code,belong_industry,linkman,phone,score from t_mapping_unit where unit_status=1
</select>
  <select id="queryHeiMingDanByUnitStatus" parameterType="com.aaa.cehui.model.MappingUnit" resultType="com.aaa.cehui.model.MappingUnit">
  <![CDATA[select id,unit_name,register_address,qualification_level,certificate_code,belong_industry,linkman,phone,score from t_mapping_unit where unit_status=2]]>
</select>

  <select id="selectAllMappingUtil" parameterType="com.aaa.cehui.model.MappingUnit" resultType="com.aaa.cehui.model.MappingUnit" >
