  </select>

  <select id="queryAllMappingUtilByName" parameterType="com.aaa.cehui.model.MappingUnit" resultType="com.aaa.cehui.model.MappingUnit" >
    select id,unit_name,register_address,qualification_level,certificate_code,belong_industry,linkman,phone,score from t_mapping_unit where unit_name like concat('%',#{name},'%')
  </select>
  <update id="updateScoreAndStatusById" parameterType="com.aaa.cehui.model.MappingUnit">
    update t_mapping_unit set score=#{score},unit_status=#{unitStatus} where id=#{id}
  </update>
  <select id="selectPartUnitByPage" parameterType="com.aaa.cehui.model.MappingUnit" resultType="com.aaa.cehui.model.MappingUnit">
  select id,unit_name,oa,qualification_level,certificate_code,belong_industry,linkman,phone,score
  from t_mapping_unit
  <where>
    1=1
    <if test="ownedDistrict != null and ownedDistrict != '' ">
